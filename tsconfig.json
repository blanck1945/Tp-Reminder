{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./public/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    //"isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
    //"strict": true,
    //"noEmit": true
  },
  "include": ["./src/**/*"]
}
